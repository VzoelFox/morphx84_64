fungsi main
    lompat start

    fmt1:
    data "Test IO: String=%s, Int=%d, Hex=%x, Char=%c\n\0"

    fmt2:
    data "Hello %s\n\0"

    str_val:
    data "Morph\0"

    start:
    ; Call 1
    mov rdi, fmt1
    mov rsi, str_val
    mov rdx, 123
    mov rcx, 255
    mov r8, 65 ; 'A'
    mov r9, 0 ; Unused
    call printmorph

    ; Call 2
    mov rdi, fmt2
    mov rsi, str_val
    call printmorph

    mov rax, 60
    mov rdi, 0
    syscall
tutup_fungsi
